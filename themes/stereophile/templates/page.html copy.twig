    {% if page.content %}
    <div class='two-col-left'>
      {{ page.content }}
    {# /.layout-content #}
    </div>
    {% endif %}

    {% if page.sidebar_right %}
      <aside class="layout-sidebar-first" role="complementary">
        <div class='two-col-right'>
        {{ page.sidebar_first }}
        </div>
      </aside>
